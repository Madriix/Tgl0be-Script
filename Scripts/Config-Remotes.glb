on *:dialog:Tgl0be:edit:*:{
  If ($did == 109) || ($did == 110) || ($did == 132) || ($did == 133) { 
    SauverEcriture
    .timer 1 0 MontrerApercu 
  }
  If ( $did > 205 ) && ( $did < 277 ) {
    Write -l1 $shortfn($MircDirTRaccourcis.tgs) Aucune Touche
    Write -l14 $shortfn($MircDirTRaccourcis.tgs) Control
    Write -l27 $shortfn($MircDirTRaccourcis.tgs) Shift
    Var %i = $Did

    If ( %i < 229 ) { Var %r = $Calc( %i - 215 ) } 
    If ( %i < 253 ) && ( %i > 240 ) {  Var %r = $Calc( %i - 226 ) }
    If ( %i > 264 ) { Var %r = $Calc( %i - 237 )  }
    If ($Did(%i) != $Null) { Write -l $+ %r $shortfn($MircDirTRaccourcis.tgs) $Did(%i) }
    Else { Write -l $+ %r $shortfn($MircDirTRaccourcis.tgs) Echo -a Aucune action associée }
    If ($Did(%i) == ReponseInvite) { TRacc Invite %r }
  }
  If ( $did == 333 ) {
    did -e Tgl0be 334 
    if ( $did(333) == $null ) { did -b Tgl0be 334 }
  }
  If ( $did == 182 ) {
    Did -u Tgl0be 188
    Did -u Tgl0be 190
    Did -u Tgl0be 192
    Did -u Tgl0be 194
    Did -r Tgl0be 187
    Did -rat Tgl0be 195 A&jouter
  }
  if ( $did == 435 ) { set %asv.age $did(435) }
  if ( $did == 438 ) { set %asv.sexe $did(438) }
  if ( $did == 440 ) { set %asv.ville $did(440) }
}
