  If ($did == 488) { %Raws.WhoIsOnQuery = $false | %Raws.WhoIsOnWindow = $false | Did -u Tgl0be 489,490 }
  If ($did == 489) { %Raws.WhoIsOnQuery = $true | %Raws.WhoIsOnWindow = $false | Did -u Tgl0be 488,490 }
  If ($did == 490) { %Raws.WhoIsOnQuery = $false | %Raws.WhoIsOnWindow = $true | Did -u Tgl0be 488,489 }
  If ($did == 492) { 
    %IrcOps.ScanConnect = $iif($did(492).state == 1,$true,$false) 
    %IrcOps.ScanChan = $iif($did(492).state == 1,$true,$false) 
    If ($did(492).state == 1) { Did -c Tgl0be 493,494 | Did -e Tgl0be 493,494 } 
    Else { Did -u Tgl0be 493,494 | Did -b Tgl0be 493,494 }
  }
  If ($did == 493) { %IrcOps.ScanConnect = $iif($did(493).state == 1,$true,$false) }
  If ($did == 494) { %IrcOps.ScanChan = $iif($did(494).state == 1,$true,$false) }
  If ($did == 496) { 
    %Clones.OnJoinChan = $iif($did(496).state == 1,Activé,Désactivé) 
    %Scan.Clone.Affiche = $iif($did(496).state == 1,Activé,Désactivé) 
    If ($did(496).state == 1) { Did -c Tgl0be 497,498 | Did -e Tgl0be 497,498 } 
    Else { Did -u Tgl0be 497,498 | Did -b Tgl0be 497,498 }
  }
  If ($did == 497) { %Clones.OnJoinChan = $iif($did(497).state == 1,Activé,Désactivé) }
  If ($did == 498) { %Scan.Clone.Affiche = $iif($did(498).state == 1,Activé,Désactivé) }
  If ($did == 502) { %Pv.Activer = $iif($did(502).state == 1,$true,$false) | If (%Pv.Activer == $true) { Did -e Tgl0be 503 | Did -f Tgl0be 503 } | Else { Did -b Tgl0be 503 } }
  If ($did == 504) { %Pv.AutoClose = $iif($did(504).state == 1,$true,$false) | If (%Pv.AutoClose == $true) { Did -e Tgl0be 505 | Did -f Tgl0be 505 } | Else { Did -b Tgl0be 505 } }
  IF ($did == 511) { RAZKickCompteur | Did -b Tgl0be 511 | Did -ra Tgl0be 511 Vous n'avez aucun Kick à votre actif. }
  IF ($did == 512) { VoirHistoKick }
  IF ($did == 514) { %Kick.RAZTime = $duration(1day) }
  IF ($did == 515) { %Kick.RAZTime = $duration(1wks) }
  IF ($did == 516) { %Kick.RAZTime = $duration(4wks) }
  IF ($did == 517) { %Kick.RAZTime = $duration(52wks) }
  IF ($did == 518) { Unset %Kick.RAZTime }
  If ( $Did == 523 ) { Did -c Tgl0be 524 $Did(523).Sel }
  If ( $Did == 524 ) { Did -c Tgl0be 523 $Did(524).Sel }
  If ( $Did == 525 ) { 
    Var %i = $Input( "Quelle commande (exemple : !re) :" , 69, "Mes Pseudos" )
    If ( %i != $Null ) { Var %j = $Input( "Quel Pseudo (exemple : JeReviens) :" , 69, "Mes Pseudos" , $Me ) }
    If ( %i != $Null ) && ( %j != $Null ) {
      Did -a Tgl0be 523 %i
      Did -a Tgl0be 524 %j
    }
  }
  If ( $Did == 526 ) { 
    Did -d Tgl0be 523 $Did(523).Sel
    Did -d Tgl0be 524 $Did(524).Sel
  }
}
