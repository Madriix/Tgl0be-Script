ON *:DIALOG:SkinsMaj:dclick:*: {
  If ($did == 201) { GestionControlTabImages $true }
  If ($did == 301) { GestionControlTabSons $true }
}
ON *:DIALOG:SkinsMaj:sclick:*: {
  If ($did >= 200) && ($did < 300) { GestionControlTabImages }
  If ($did >= 300) && ($did < 400) { GestionControlTabSons }
  If ($did >= 500) && ($did < 600) { GestionControlTabEvents }
  IF ($did >= 600) && ($did < 700) { GestionControlTabNicklist } 
  If ($did == 10) {
    If $did(101) != $null {
      Var %RepSkin = $skindir $+ \ $+ $did(101), %SkinIni = $shortfn( %RepSkin $+ \ $+ Skin.ini )
      %Skin.Fenetres.EditBox.Text  = $did(412).sel
      %Skin.Fenetres.EditBox.Fond  = $did(414).sel
      %Skin.Fenetres.Fenetre.Fond  = $did(424).sel
      %Skin.Fenetres.NickList.Fond = $did(434).sel
      EnregistreSkin %RepSkin %SkinIni
      RefreshSkinList
    }
    Else { 
      Var %R = $input(Impossible d'enregistrer. Vous n'avez pas indiqué le nom du skin.,260)
      Did -f SkinsMaj 101
    }
  }
  If (($did == 10) || ($did == 20)) { 
    If ($did == 10) && (%Skin.Nom == %Skin.Defaut) {
      Var %Recharger = $input(Désirez-vous charger les modifications apportées au skin ?,136)
      If (%Recharger == $true) { ChargeSkin %Skin.Defaut }
    }
    DechargeVarSkin
    Dialog -x SkinsMaj
  }
}
