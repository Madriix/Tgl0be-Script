On 1:Dialog:Tgl0be:Init:*: {
  If ( $Away == $True ) { Did -ra Tgl0be 41 &Désactiver }
  Var %i = 1
  Did -r Tgl0be 44
  While ( $ReadIni $MircDirAway.ini Nick %i != $Null ) {
    Did -a Tgl0be 44 $ReadIni $MircDirAway.ini Nick %i
    Inc %i
  }
  Var %i = 1
  Did -r Tgl0be 46
  While ( $ReadIni $MircDirAway.ini Raison %i != $Null ) {
    Did -a Tgl0be 46 $ReadIni $MircDirAway.ini Raison %i
    Inc %i
  }
  Did -c Tgl0be 44 $ReadIni $MircDirAway.ini Defaut Nick
  Did -c Tgl0be 46 $ReadIni $MircDirAway.ini Defaut Raison
  If ( $ReadIni $MircDirAway.ini Config ChangeNick == 1 ) { Did -c Tgl0be 43 }
  Else { Did -u Tgl0be 43 }
  If ( $ReadIni $MircDirAway.ini Auto Activer == 1 ) { Did -c Tgl0be 48 }
  Else { Did -u Tgl0be 48 }
  Var %i = $ReadIni $MircDirAway.ini Auto Idle
  Did -ra Tgl0be 49 $Calc(%i / 60)
  Did -ra Tgl0be 52 $ReadIni $MircDirAway.ini Auto Raison
  If ( $ReadIni $MircDirAway.ini Temps Depart == 1 ) { Did -c Tgl0be 56 }
  Else { Did -u Tgl0be 56 }
  If ( $ReadIni $MircDirAway.ini Temps Repet == 1 ) { Did -c Tgl0be 57 }
  Else { Did -u Tgl0be 57 }
  Did -ra Tgl0be 58 $ReadIni $MircDirAway.ini Temps TempsRepet
  If ( $ReadIni $MircDirAway.ini Temps Retour == 1 ) { Did -c Tgl0be 60 }
  Else { Did -u Tgl0be 60 }
  Did -ra Tgl0be 65 $ReadIni $MircDirAway.ini Raccourcis Départ
  Did -ra Tgl0be 67 $ReadIni $MircDirAway.ini Raccourcis Retour
  Did -r Tgl0be 77
  Var %i = 1
  While ( %i <= $findfile( $MircDirRaison , *.gmp ,  0 ) ) {
    Did -a Tgl0be 76 $NoPath( $findfile( $MircDirRaison , *.gmp ,  %i ) )
    Inc %i
