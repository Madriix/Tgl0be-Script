On *:Dialog:Tgl0be:Sclick:*: {
  If ( $Did == 42 ) {
    SauveAwayParam
    Unset %ecriture.type
    Window -c @Apercu
    .timerpowwin off
    Var %i = 1
    Unset %bot.*
    While (%i <= $Did(454).lines) {
      Did -c Tgl0be 454 %i
      Set %bot.pseudo [ $+ [ %i ] ] $did(454).seltext
      Inc %i
    }
    Set %CmdesBot.op $did(463)
    Set %CmdesBot.deop $did(465)
    Set %CmdesBot.voice $did(467)
    Set %CmdesBot.devoice $did(469)
    Set %CmdesBot.kick $did(471)
    Set %CmdesBot.ban $did(473)
    Set %CmdesBot.deban $did(475)
    Set %CmdesBot.kickban $did(477)
    If ($did(283).state == 1) { Set %protect.check.ignore Activé }
    Else { Set %protect.check.ignore Désactivé }
    If ($did(286).state == 1) { Set %protect.etat.flood Activé }
    Else { Set %protect.etat.flood Désactivé }
    set %protect.flood.text.lim $did(288)
    set %protect.flood.text.timer $did(290) 
    If ($did(296).state == 1) { Set %protect.etat.majs Activé }
